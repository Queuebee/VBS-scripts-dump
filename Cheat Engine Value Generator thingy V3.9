'Cheat Engine Value-generator V3.9'




set shell = createobject("wscript.shell")

	MsgBox"Welcome to The Value-Gen 1.0"

	'string origins'

	'Amount of wanted generator iterations'
	CountMax = inputBox("How many values would you like to generate?")
			if not isnumeric(strtimes) then
				lol = msgbox("That is NaN. Script end.")
				wscript.quit
			end if
			
			if CountMax >=100 then
				Msgbox"Too much, can't handle! Forcequit!"
				wscript.quit
			end if
				
	
	'Starting Value'
	StartValue = inputBox("What is your starting Value?")
			if not isnumeric(strtimes) then
				Mmsgbox("That is NaN. Script end.")
				wscript.quit
			end if	
			
	'Increment per iteration'	
	Increment = inputBox("What is change of value per Increment(Decrement not accepted)?")
			if not isnumeric(strtimes) then
				Msgbox("That is NaN. Script end.")
				wscript.quit
			end if	
			
	'Multiplier'	
	Multiplier = inputBox("With which number would you like to multiply? (1 for no multiplication)")	
			if not isnumeric(strtimes) then
				MsgBox("That is NaN. Script end.")	
				wscript.quit
			end if
			if Multiplier = "" then
				MsgBox("That is NaN. Script end.")	
				wscript.quit
			end if
	'Last Value'		
	
	'LastValue = Value*Multiplier'
	
	
	'Delay per output'
	Delay = inputbox ("How much delay would you like inbetween generated values? (10ms recommended)")
		if Delay >=10000 then
		msgbox "This script is not meant to run slow, have a nice day"
		wscript.quit
		end if
	
	
	MsgBox "All Settings are now set. Press OK to start script (5 second delay till start)"
	wscript.sleep(5000)
	
	'settings log'
	
	Shell.Sendkeys("Settings Chosen:")
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{TAB}"
	Shell.SendKeys("Number of iterations: " & CountMax)
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{TAB}"
	Shell.SendKeys(" Increment: " & Increment)
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{TAB}"
	Shell.SendKeys("StartValue: " & StartValue)
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{TAB}"
	Shell.SendKeys("Multiplier: " & Multiplier)
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{TAB}"
	Shell.SendKeys("Delay:" & Delay & "(ms)")
	Shell.SendKeys "{Enter}"
	Shell.SendKeys "{Enter}"
	
	
	for Count = 0 to CountMax
	
		Value = Count*Increment + StartValue 
		
		Shell.Sendkeys("#" & Count)
		Shell.Sendkeys"{TAB}"
		Shell.Sendkeys(Value)
		Shell.Sendkeys"{TAB}"
		Shell.Sendkeys(Value*Multiplier)
		Shell.SendKeys "{Enter}"

		wscript.sleep(Delay)

		
	next
	

	Shell.Sendkeys("This Demo was programmed by QB Coorporation Boulevard Technologies incoorporated zooi manufacturers")
	Shell.SendKeys "{Enter}"
